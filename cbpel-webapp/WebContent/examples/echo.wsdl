<?xml version="1.0" encoding="ISO-8859-1"?>

<wsdl:definitions
  targetNamespace="http://beepell.com/samples/echo/definitions"
  xmlns:srv="http://beepell.com/samples/proxy/definitions"
  xmlns:plnk="http://docs.oasis-open.org/wsbpel/2.0/plnktype"
  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <plnk:partnerLinkType name="echoPartnerLinkType">
    <plnk:role name="echoServiceProvider" portType="srv:echoPortType" />
  </plnk:partnerLinkType>

  <wsdl:message name="echoMessage">
    <wsdl:part name="content" type="xsd:string" />
  </wsdl:message>

  <wsdl:portType name="echoPortType">
    <wsdl:operation name="echoOperation">
      <wsdl:input message="srv:echoMessage" />
      <wsdl:output message="srv:echoMessage" />
    </wsdl:operation>
  </wsdl:portType>

</wsdl:definitions>
