sigs = info-sig bgterm-sig bgval-sig bgbdnf-sig rule-sig sugar-sig
       instantiation-sig
funs = info bgterm bgval bgbdnf sugar
       instantiation rule

info-sig: /lib/util/origin
info: info-sig /lib/util/origin %NODASHSIG
bgterm-sig: /lib/pp/smlnj/prettyprint
bgterm: info-sig /lib/pp/smlnj/. /lib/coll/pp /lib/coll/set /kernel/bg/sigs
bgval-sig: /lib/pp/smlnj/prettyprint
bgval: info-sig /lib/pp/smlnj/. /lib/coll/monoset-sig /lib/coll/pp
   bgterm-sig /kernel/bg/sigs /lib/util/errorhandler-sig
   /lib/util/origin /lib/util/debug /lib/util/flags
   /lib/compat/mosml/hash/.
bgbdnf-sig: /lib/pp/smlnj/prettyprint
bgbdnf: info-sig /lib/coll/monoset-sig /lib/coll/pp /kernel/bg/sigs
   bgval-sig /lib/pp/smlnj/. /lib/util/errorhandler-sig
   /lib/util/origin /lib/util/debug /lib/util/flags
#ppbg bgterm-sig /lib/pp/smlnj/. %NODASHSIG
sugar-sig:
sugar: info-sig /lib/coll/monoset-sig /lib/coll/pp /kernel/bg/sigs bgval-sig
   /lib/util/errorhandler-sig /lib/util/origin /lib/util/debug
   /lib/pp/smlnj/.
instantiation-sig: /lib/pp/smlnj/prettyprint
instantiation: instantiation-sig /lib/pp/smlnj/. bgval-sig bgbdnf-sig
  /lib/util/flags %NODASHSIG
rule-sig: /lib/pp/smlnj/prettyprint
rule: /lib/pp/smlnj/. instantiation-sig bgval-sig bgbdnf-sig rule-sig
  /lib/util/flags %NODASHSIG
