. = bg.grm bg.lex rules.grm rules.lex
bg.grm-sig: /lib/ml-yacc/.
bg.lex: /lib/util/errormsg /lib/compat/mosml/. bg.grm-sig
bg.grm: /lib/ml-yacc/.  /lib/coll/monoset-sig bg.grm-sig
   /kernel/bg/sigs /kernel/ast/sigs /lib/util/errormsg
   /lib/util/origin-sig /lib/util/origin /lib/pp/smlnj/. %NODASHSIG
rules.grm-sig: /lib/ml-yacc/.
rules.lex: /lib/util/errormsg /lib/compat/mosml/. rules.grm-sig
rules.grm: /lib/ml-yacc/.  /lib/coll/monoset-sig rules.grm-sig
   /kernel/bg/sigs /kernel/ast/sigs /kernel/match/sigs /lib/util/errormsg
   /lib/util/origin-sig /lib/util/origin /lib/pp/smlnj/. %NODASHSIG

bpl.grm-sig: /lib/ml-yacc/.
bpl.lex: bpl.grm-sig /lib/util/sourceloc /lib/compat/mosml/. /lib/pp/mak/.
bpl.grm: bpl.grm-sig /kernel/ast/bplterm
   /lib/util/sourceloc /lib/ml-yacc/.
   %NODASHSIG
bplparser: bpl.grm bpl.lex /kernel/ast/bplterm
   /lib/ml-yacc/. /lib/pp/mak/. /lib/util/sourceloc
   %PROGRAM