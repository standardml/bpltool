datatype int = Z | S of int
fun iplus (i1,i2) =
    case i1 of
	Z => i2
      | S i => S(iplus(i,i2))
datatype 'a list = Cons of 'a * 'a list | Nil
val one = Cons( (), Nil)
val length = 
  fix l' xs => case xs of
            	 [] => 0
               | y::ys => 1 + l' ys
val l = length one
val l' = length (Cons (17, Nil))