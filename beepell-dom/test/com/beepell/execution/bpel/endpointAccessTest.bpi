<?xml version="1.0" encoding="UTF-8"?>

<instance 
  bpi:state="running"
  name="exchange-instance"
  targetNamespace="http://beepell.com/tests/context/links"  
  xmlns:exc="http://beepell.com/tests/exchange/wsdl"
  xmlns="http://www.itu.dk/research/pls/bpl/bpel/instance"
  xmlns:bpi="http://www.itu.dk/research/pls/bpl/bpel/instance"
  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/">

  <partnerLinks>
    <partnerLink
      name="echoPartnerLink"
      partnerLinkType="srv:echoPartnerLinkType"
      myRole="echoProviderRole">
      <myRoleEndpoint>
        <wsdl:port name="echoPartnerLink.echoProviderRole" binding="ws:echoBinding">
          <soap:address location="http://localhost:8080/test/echo"></soap:address>
        </wsdl:port>
      </myRoleEndpoint>  
    </partnerLink>
    <partnerLink
      name="insurancePartnerLink"
      partnerLinkType="tim:insurancePartnerLinkType"
      partnerRole="insuranceProviderRole"
      initializePartnerRole="no" />    
  </partnerLinks>
  
  <sequence>
  
    <empty name="A" />
    <scope name="S">
      <partnerLinks>
        <partnerLink
          name="insurancePartnerLink"
          partnerLinkType="tim:insurancePartnerLinkType"
          partnerRole="insuranceProviderRole"
          initializePartnerRole="no">
          <partnerRoleEndpoint>
            <wsdl:port name="echoPartnerLink.echoProviderRole" binding="ws:echoBinding">
              <soap:address location="http://remotehost:8080/test/insurance"></soap:address>
            </wsdl:port>
          </partnerRoleEndpoint> 
        </partnerLink>  
      </partnerLinks>
      <empty name="B" />
    </scope>
  
  </sequence>
  
</instance>